@using GidaGkpWeb.Infrastructure.Authentication;
@using GidaGkpWeb.Models;

@{
    ViewBag.Title = "ApplicantDashboard";
    Layout = "~/Views/Shared/_ApplicantLayout.cshtml";

    //var UserData = ViewData["UserData"] as List<ApplicationDetailModel>;
    //int index = 1;
}
<script src="~/Scripts/App/Masters/ApplicantPlotRegistration.js"></script>
<link href="~/styles/style.css" rel="stylesheet" />
<section class="form">
    <!-- MultiStep Form -->
    <div class="container-fluid" id="grad1">
        <div class="container">
            <div class="row justify-content-center mt-0" style="padding:30px 20px" id="divNewPlotRegistration">
                <div class="col-sm-12 col-xs-12">
                    <div class="card px-0 pt-4 pb-0 mt-3 mb-3">
                        <div class="col-sm-12 col-xs-12">
                            <div class="col-sm-6 col-xs-6">
                                <h2><strong>Plot Details</strong></h2>
                                <p>Fill all form field to go to next step</p>
                            </div>
                            <div class="col-sm-6 col-xs-6" id="divApplicationNumber" style="display:none;background-color:burlywood">
                                Application Number
                                <span id="spanApplicationNumber"></span>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-12 mx-0">
                                <div id="msform">
                                    <!-- progressbar -->
                                    <ul id="progressbar">
                                        <li class="active" id="PlotDetail"><strong>Plot Details</strong></li>
                                        <li id="ApplicantDetail"><strong>Applicant Details</strong></li>
                                        <li id="ProjectDetail"><strong>Project Details</strong></li>
                                        <li id="BankDetail"><strong>Bank Details</strong></li>
                                        <li id="AttachDocument"><strong>Attach Documents</strong></li>
                                        @*<li id="MakePayment"><strong>Make Payment</strong></li>*@
                                    </ul>
                                    <!-- Step1 fieldsets -->
                                    <fieldset>
                                        <form>
                                            <div class="form-card">
                                                <h2 class="fs-title">Plot Details</h2>
                                                <div class="col-sm-12 col-xs-12" style="padding:20px 0px;">
                                                    <div class="row">
                                                        <div class="form-group col-sm-12 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Applied For<span class="required-field"></span></label>
                                                            <select name="AppliedFor" id="AppliedFor" class="form-control" required>
                                                            </select>
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Scheme Type<span class="required-field"></span></label>
                                                            <select name="SchemeType" id="SchemeType" class="form-control">
                                                            </select>
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Plot Range (In Sqr Mtr.)<span class="required-field"></span></label>
                                                            <select name="PlotRange" id="PlotRange" class="form-control">
                                                            </select>

                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Scheme Name<span class="required-field"></span></label>
                                                            <select name="SchemeName" id="SchemeName" class="form-control">
                                                            </select>
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Plot Area (In Sqr Mtr.)<span class="required-field"></span></label>
                                                            <input name="plotArea" id="plotArea" class="form-control">
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Sector Name<span class="required-field"></span></label>
                                                            <select name="SectorName" id="SectorName" class="form-control">
                                                            </select>
                                                            <input type="hidden" id="SectorDescription" name="SectorDescription" />
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Estimated Rate (In Rs.)</label>
                                                            <input name="EstimatedRate" id="EstimatedRate" class="form-control" readonly>
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Paymemt Schedule<span class="required-field"></span></label>
                                                            <select name="PaymemtSchedule" id="PaymemtSchedule" class="form-control">
                                                            </select>
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Total Investment (In Rs.)</label>
                                                            <input name="TotalInvestment" id="TotalInvestment" class="form-control" readonly>
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Application Fee (In Rs.)</label>
                                                            <input name="ApplicationFee" id="ApplicationFee" class="form-control" value="1000" readonly>
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Earnest Money Deposite (In Rs.)</label>
                                                            <input name="EarnestMoneyDeposite" id="EarnestMoneyDeposite" class="form-control" readonly>
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">GST (In Rs.)</label>
                                                            <input name="GST" id="GST" class="form-control" value="50" readonly>
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Net Amount(Payable In Rs.)</label>
                                                            <input name="NetAmount" id="NetAmount" class="form-control" readonly>
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Total Amount (In Rs.)</label>
                                                            <input name="TotalAmount" id="TotalAmount" class="form-control" value="1050" readonly>
                                                        </div>
                                                    </div>
                                                </div>

                                                <h2 class="fs-title">Full Particular of the applicant</h2>
                                                <div class="col-sm-12 col-xs-12" style="padding:20px 0px;">
                                                    <div class="row">
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Industry Ownership Type<span class="required-field"></span></label>
                                                            <select name="IndustryOwnershipType" id="IndustryOwnershipType" class="form-control">
                                                            </select>
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Unit Name<span class="required-field"></span></label>
                                                            <input name="UnitName" id="UnitName" class="form-control">
                                                        </div>
                                                    </div>
                                                </div>

                                                <h2 class="fs-title">Signing Person Detail</h2>
                                                <div class="col-sm-12 col-xs-12" style="padding:20px 0px;">
                                                    <div class="row">
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Name<span class="required-field"></span></label>
                                                            <input name="Name" id="Name" class="form-control">
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Date of Birth<span class="required-field"></span></label>
                                                            <input name="dob" type="date" id="dob" class="form-control" placeholder="Date of Birth">
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Present Address<span class="required-field"></span></label>
                                                            <textarea name="PresentAddress" id="PresentAddress" class="form-control"></textarea>
                                                        </div>

                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Permanent Address and Contact No<span class="required-field"></span></label>
                                                            <textarea name="PermanentAddress" id="PermanentAddress" class="form-control"></textarea>
                                                        </div>

                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Relationship Status<span class="required-field"></span></label>
                                                            <select name="RelationshipStatus" id="RelationshipStatus" class="form-control">
                                                            </select>
                                                        </div>

                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-sm-12 col-xs-12">
                                                <input type="button" id="btnPlotDetailSave" name="next" class="next action-button pull-right" value="Save & Next" />
                                                <input type="button" id="btnStep1Skip" name="next" class="next action-button pull-right hidden" value="Next" />
                                            </div>

                                        </form>
                                    </fieldset>
                                    <!-- Step2 fieldsets -->
                                    <fieldset>
                                        <form>
                                            <div class="form-card">
                                                <h3 class="fs-title">Applicant Peronal Details</h3>
                                                <div class="col-sm-12 col-xs-12" style="padding:20px 0px;">
                                                    <div class="row">
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Full Name of Applicant<span class="required-field"></span></label>
                                                            <input name="FullName" id="FullName" class="form-control">
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Father's Name<span class="required-field"></span></label>
                                                            <input name="FatherName" id="FName" class="form-control">
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Mother's Name<span class="required-field"></span></label>
                                                            <input name="MotherName" id="MName" class="form-control">
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Spouce Name</label>
                                                            <input name="SpouceName" id="SName" class="form-control">
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Date of Birth<span class="required-field"></span></label>
                                                            <input name="DOB" type="date" id="DOB" class="form-control">
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Gender<span class="required-field"></span></label>
                                                            <select name="Gender" id="Gender" class="form-control">
                                                                <option value="">Select</option>
                                                                <option value="Male">Male</option>
                                                                <option value="Female">Female</option>
                                                            </select>
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Category<span class="required-field"></span></label>
                                                            <select name="Category" id="Category" class="form-control">
                                                                <option value="">Select</option>
                                                                <option value="General">General</option>
                                                                <option value="OBC">OBC</option>
                                                                <option value="SC">SC</option>
                                                                <option value="ST">ST</option>
                                                                <option value="Others">others</option>
                                                            </select>
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Nationality<span class="required-field"></span></label>
                                                            <select name="Nationality" id="Nationality" class="form-control">
                                                                <option value="Indian">Indian</option>
                                                                <option value="Others">others</option>
                                                            </select>
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Adhaar Number</label>
                                                            <input name="AdhaarNo" id="AdhaarNo" class="form-control">
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">PAN Number<span class="required-field"></span></label>
                                                            <input name="PAN" id="PAN" class="form-control" value="">
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Mobile Number<span class="required-field"></span></label>
                                                            <input name="MobileNo" id="MobileNo" class="form-control">
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Phone Number</label>
                                                            <input name="Phone" id="Phone" class="form-control">
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Email ID<span class="required-field"></span></label>
                                                            <input name="Email" id="Email" class="form-control">
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Religion<span class="required-field"></span></label>
                                                            <select name="Religion" id="Religion" class="form-control">
                                                                <option value="">Select</option>
                                                                <option value="Hindu">Hindu</option>
                                                                <option value="Muslim">Muslim</option>
                                                                <option value="Sikh">Sikh</option>
                                                                <option value="Others">others</option>
                                                            </select>
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Sub-Category</label>
                                                            <input name="SubCategory" id="SubCategory" class="form-control">
                                                        </div>
                                                    </div>
                                                </div>
                                                <h3 class="fs-title">Applicant Identity Details</h3>
                                                <div class="form-group col-sm-6 col-xs-12">
                                                    <label for="usertype" class="text-uppercase">Current Address<span class="required-field"></span></label>
                                                    <input name="CAddress" id="CAddress" class="form-control">
                                                </div>
                                                <div class="form-group col-sm-6 col-xs-12">
                                                    <label for="usertype" class="text-uppercase">Permanent Address<span class="required-field"></span></label>
                                                    <input name="PAddress" id="PAddress" class="form-control">
                                                </div>

                                                <h3 class="fs-title">Applicant Identity Proof<span class="required-field"></span></h3>
                                                <div class="form-check col-sm-12 col-xs-12" style="padding-top:10px;">
                                                    <div class="row">
                                                        <div class="col-sm-4">
                                                            <input type="radio" id="IDPassport" name="IdentityProof" value="Passport">
                                                            <label for="Passport">Passport</label>
                                                        </div>
                                                        <div class="col-sm-4">
                                                            <input type="radio" id="IDPAN" name="IdentityProof" value="PAN">
                                                            <label for="PAN">PAN</label>
                                                        </div>
                                                        <div class="col-sm-4">
                                                            <input type="radio" id="IDvoterIDCard" name="IdentityProof" value="Oter ID Card">
                                                            <label for="OterIDCard">Voter ID Card</label>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-sm-4">
                                                            <input type="radio" id="IDDrivingLiecence" name="IdentityProof" value="Driving Liecence">
                                                            <label for="DrivingLiecence">Driving Liecence</label>
                                                        </div>
                                                        <div class="col-sm-4">
                                                            <input type="radio" id="IDAdhaarCard" name="IdentityProof" value="AdhaarCard">
                                                            <label for="AdhaarCard">Adhaar Card</label>
                                                        </div>
                                                        <div class="col-sm-4">
                                                            <input type="radio" id="IDCompanyIDCard" name="IdentityProof" value="Company ID Card">
                                                            <label for="CompanyIDCard">Company ID Card</label>
                                                        </div>
                                                    </div>
                                                </div>

                                                <h3 class="fs-title">Applicant Residential Proof<span class="required-field"></span></h3>
                                                <div class="form-check col-sm-12 col-xs-12" style="padding-top:10px;">
                                                    <div class="row">
                                                        <div class="col-sm-3">
                                                            <input type="radio" id="RPElectricBill" name="ResidentialProof" value="Electric Bill">
                                                            <label for="ElectricBill">Electric Bill</label>
                                                        </div>
                                                        <div class="col-sm-3">
                                                            <input type="radio" id="RPITR" name="ResidentialProof" value="ITR">
                                                            <label for="ITR">ITR</label>
                                                        </div>
                                                        <div class="col-sm-3">
                                                            <input type="radio" id="RPTelephoneBill" name="ResidentialProof" value="Telephone Bill">
                                                            <label for="TelephoneBill">Telephone Bill</label>
                                                        </div>
                                                        <div class="col-sm-3">
                                                            <input type="radio" id="RPBankPassbook" name="ResidentialProof" value="Bank Passbook Account Detail">
                                                            <label for="BankPassbook">Bank Passbook Account Detail</label>
                                                        </div>
                                                        <div class="col-sm-3">
                                                            <input type="radio" id="RPPassport" name="ResidentialProof" value="Passport">
                                                            <label for="Passport">Passport</label>
                                                        </div>
                                                        <div class="col-sm-3">
                                                            <input type="radio" id="RPVoterIDCard" name="ResidentialProof" value="Voter ID Card">
                                                            <label for="VoterIDCard">Voter ID Card</label>
                                                        </div>
                                                        <div class="col-sm-3">
                                                            <input type="radio" id="RPHRBill" name="ResidentialProof" value="HR Bill">
                                                            <label for="HRBill">HR Bill</label>
                                                        </div>
                                                        <div class="col-sm-3">
                                                            <input type="radio" id="RPDrivingLiecence" name="ResidentialProof" value="Driving Liecence">
                                                            <label for="DrivingLiecence">Driving Liecence</label>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-sm-12 col-xs-12">
                                                <input type="button" name="previous" id="Step2PreviousButton" class="previous action-button-previous pull-right" value="Previous" />
                                                <input type="button" id="btnStep2Skip" name="next" class="next action-button pull-right hidden" value="Next" />
                                                <input type="button" name="next" id="Step2NextButton" class="next action-button pull-right" value="Save & Next" />
                                            </div>
                                        </form>
                                    </fieldset>
                                    <!-- Step3 fieldsets -->
                                    <fieldset>
                                        <form>
                                            <div class="form-card">
                                                <h3 class="fs-title">Particular Project Details</h3>
                                                <div class="col-sm-12 col-xs-12" style="padding:20px 0px;">
                                                    <div class="row">
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Type of Industry Proposed to be Set up<span class="required-field"></span></label>
                                                            <input name="ProposedIndustryType" id="ProposedIndustryType" class="form-control">
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Estimated Cost of The Project (in RS)<span class="required-field"></span></label>
                                                            <input name="ProjectEstimatedCost" id="ProjectEstimatedCost" class="form-control">
                                                        </div>
                                                    </div>
                                                </div>

                                                <h3>Proposed Layout Plan of Land</h3>
                                                <div class="col-sm-12 col-xs-12" style="padding:20px 0px;">
                                                    <div class="row">
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Covered Area (In Sqr Mtr.)<span class="required-field"></span></label>
                                                            <input name="MotherName" id="CoveredArea" class="form-control">
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Open Area (Sqr Mtr.)<span class="required-field"></span></label>
                                                            <input name="SpouceName" id="OpenArea" class="form-control">
                                                        </div>
                                                        <div class="form-group col-sm-12 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Purpose for open area (In Sqr Mtr.)<span class="required-field"></span></label>
                                                            <input name="DOB" id="Purposeforopenarea" class="form-control">
                                                        </div>
                                                    </div>
                                                </div>

                                                <h3>Deatails of Proposed Investment</h3>
                                                <div class="col-sm-12 col-xs-12" style="padding:20px 0px;">
                                                    <div class="row">
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Investment in land (In RS)<span class="required-field"></span></label>
                                                            <input name="MotherName" id="Investmentland" class="form-control">
                                                        </div>
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Investment in Building & Factory Shed( In Rs )<span class="required-field"></span></label>
                                                            <input name="SpouceName" id="InvestmentBuilding" class="form-control">
                                                        </div>
                                                        <div class="form-group col-sm-12 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Investment in Plant, Machinery & Equipment (in Rs.)<span class="required-field"></span></label>
                                                            <input name="DOB" id="InvestmentPlant" class="form-control">
                                                        </div>
                                                    </div>
                                                </div>

                                                <h3>Fumes & Industrial Effluents Detail</h3>
                                                <div class="col-sm-12 col-xs-12" style="padding:20px 0px;">
                                                    <div class="row">
                                                        <div class="form-group col-sm-12 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Would any fumes be generated in the process of manufacture and if so,their nature and quantity<span class="required-field"></span></label>
                                                            <input name="MotherName" id="processofmanufacture" class="form-control">
                                                        </div>
                                                        <div class="form-group col-sm-12 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Industrial Efflunets</label>
                                                            <table class="table table-bordered responsive">
                                                                <thead>
                                                                    <tr>
                                                                        <th>#</th>
                                                                        <th>Name of the Effluent</th>
                                                                        <th>Quantity</th>
                                                                        <th>Chemical Composition</th>
                                                                    </tr>
                                                                </thead>
                                                                <tbody>
                                                                    <tr>
                                                                        <th row-header="Table heading" scope="row">1</th>
                                                                        <td row-header="Table">Liquid</td>
                                                                        <td row-header="Table heading"><input name="SpouceName" id="LiquidQuantity" class="form-control"></td>
                                                                        <td row-header="heading"><input name="SpouceName" id="LiquidChemicalComposition" class="form-control"></td>
                                                                    </tr>
                                                                    <tr>
                                                                        <th row-header="Table heading" scope="row">2</th>
                                                                        <td row-header="Table heading">Solid</td>
                                                                        <td row-header="Table heading"><input name="SpouceName" id="SolidQuantity" class="form-control"></td>
                                                                        <td row-header="Table heading"><input name="SpouceName" id="SolidChemicalComposition" class="form-control"></td>
                                                                    </tr>
                                                                    <tr>
                                                                        <th row-header="Table heading" scope="row">3</th>
                                                                        <td row-header="Table heading">Gas</td>
                                                                        <td row-header="Table heading"><input name="SpouceName" id="GasQuantity" class="form-control"></td>
                                                                        <td row-header="Table heading"><input name="SpouceName" id="GasChemicalComposition" class="form-control"></td>
                                                                    </tr>
                                                                </tbody>
                                                            </table>
                                                        </div>
                                                    </div>
                                                </div>

                                                <h3>Industry Requirements</h3>
                                                <div class="col-sm-12 col-xs-12" style="padding:20px 0px;">
                                                    <div class="row">
                                                        <div class="form-group col-sm-6 col-xs-12">
                                                            <label for="usertype" class="text-uppercase">Power Requirement(in KW)</label>
                                                            <input name="MotherName" id="PowerRequirement" class="form-control">
                                                        </div>
                                                        <div class="form-group col-sm-12 col-xs-12">
