                                                            <label for="usertype" class="text-uppercase">Industrial Efflunets</label>
                                                            <table class="table table-bordered responsive">
                                                                <thead>
                                                                    <tr>
                                                                        <th>#</th>
                                                                        <th>Name of the Effluent</th>
                                                                        <th>Quantity</th>
                                                                        <th>Chemical Composition</th>
                                                                    </tr>
                                                                </thead>
                                                                <tbody>
                                                                    <tr>
                                                                        <th row-header="Table heading" scope="row">1</th>
                                                                        <td row-header="Table">Liquid</td>
                                                                        <td row-header="Table heading"><input name="SpouceName" id="LiquidQuantity" class="form-control"></td>
                                                                        <td row-header="heading"><input name="SpouceName" id="LiquidChemicalComposition" class="form-control"></td>
                                                                    </tr>
                                                                    <tr>
