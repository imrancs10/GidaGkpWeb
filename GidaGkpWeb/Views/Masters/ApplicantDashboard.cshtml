                                                                <option value="Married">Married</option>
                                                                <option value="UnMarried">UnMarried</option>
                                                                <option value="Devorcy">Devorcy</option>
                                                            </select>
                                                        </div>

                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-sm-12 col-xs-12">
                                                <input type="button" id="btnPlotDetailSave" name="next" class="next action-button pull-right" value="Save & Next" />
                                            </div>
                                            
    @*</form>*@
                                        </fieldset>
                                        <!-- Step2 fieldsets -->
