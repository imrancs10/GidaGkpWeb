                                        </fieldset>
                                        <!-- Step3 fieldsets -->
                                        <fieldset>
                                            <div class="form-card">
                                                <h3 class="fs-title">Perticular Project Details</h3>
                                                <div class="col-sm-12 col-xs-12" style="padding:20px 0px;">
                                                    <div class="row">
                                                        <div class="form-group col-sm-6 col-xs-12">
